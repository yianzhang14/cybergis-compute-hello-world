{
    "name": "hello world",
    "container": "python",
    "pre_processing_stage": "python setup.py",
    "execution_stage": "bash main.sh",
    "post_processing_stage": "python cleanup.py",
    "slurm_input_rules": {
        "time": {
            "max": 50,
            "min": 10,
            "default_value": 10,
            "step": 1,
            "unit": "Minutes"
        },
        "num_of_task": {
            "max": 6,
            "min": 1,
            "default_value": 2,
            "step": 1
        }
    },
    "require_upload_data": false,
    "param_rules": {
        "input_a": {
            "type": "integer",
            "require": true,
            "max": 100,
            "min": 0,
            "default_value": 50,
            "step": 10
        },
        "input_b": {
            "type": "string_option",
            "options": ["foo", "bar"],
            "default_value": "foo"
        }
    },
    "default_result_folder_downloadable_path": "/demo_quick_start_0",
    "supported_hpc": ["aces_community", "anvil_community", "expanse_community", "keeling_community"],
    "default_hpc": "anvil_community"
}
